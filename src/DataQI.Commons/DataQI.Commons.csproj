    <PackageTags>Data Query Interface Commons</PackageTags>
    <PackageProjectUrl>https://github.com/henrique-gouveia/DataQI.Commons</PackageProjectUrl>
    <Product />
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
